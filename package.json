{
  "name": "line-bot-nodejs-starter",
  "version": "1.0.0",
  "description": "starter point to create new line bot project",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./dist/index.js",
    "nodemon": "nodemon ./src/index.js --exec babel-node -e js",
    "build": "npm run clean && mkdir dist && babel src -s -d dist --copy-files",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "line",
    "bot",
    "messaging-api"
  ],
  "author": "Witthawin Sirisiwaphak",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0"
  },
  "dependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@line/bot-sdk": "^6.8.4",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "twitter": "^1.7.1",
    "twitter-lite": "^0.9.4"
  }
}
